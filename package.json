    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "jest-environment-node": "29.7.0",
    "typescript": "^5.8.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadanddani/MCP.git"
  },
  "bugs": {
    "url": "https://github.com/deadanddani/MCP/issues"
  },
  "homepage": "https://github.com/deadanddani/MCP#readme",
  "license": "ISC",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
